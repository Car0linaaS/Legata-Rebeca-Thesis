property {
    define {
        PowerDriven = (s1.vessel_type_powerdriven == true);
        Under12Meters = (s1.ship_length < 12);
        AllroundWhiteOn = (s1.allround_light_on == true);
        SideLightOn = (s1.side_light_on == true);
    }

    Assertion {
        COLREG_Rule23_d_i: (PowerDriven && Under12Meters) == (AllroundWhiteOn && SideLightOn);
    }
}