property {
    define {
        PowerDriven = (vessel_type_powerdriven == true);
        Under12Meters = (ship_length < 12);
        AllroundWhiteOn = (allround_light_on == true);
        SideLightOn = (side_light_on == true);
    }

    Assertion {
        COLREG_Rule23_d_i: (PowerDriven && Under12Meters) == (AllroundWhiteOn && SideLightOn);
    }
}